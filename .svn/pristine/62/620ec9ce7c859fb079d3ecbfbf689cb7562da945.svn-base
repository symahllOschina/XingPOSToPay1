package com.wanding.xingpos.bean;


import java.io.Serializable;
import java.util.List;

/**
 * 会员消费记录查询返回参数实体
 */
public class PosMemConsumeRecodeResData implements Serializable {

    private int total;
    private List<PosMemConsumeRecodeDetail> records;


    public PosMemConsumeRecodeResData() {
    }

    public int getTotal() {
        return total;
    }

    public void setTotal(int total) {
        this.total = total;
    }

    public List<PosMemConsumeRecodeDetail> getRecords() {
        return records;
    }

    public void setRecords(List<PosMemConsumeRecodeDetail> records) {
        this.records = records;
    }
}
