<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@color/white_f3f7fd"
    >
    <include layout="@layout/menu_title_layout"/>
    <LinearLayout
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:orientation="vertical"
        >

        <View style="@style/divider_line1dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white_ffffff"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >
            <TextView
                android:id="@+id/deposit_recode_details_tvMemCodeTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="会员卡号"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/deposit_recode_details_tvMemCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:gravity="right"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/deposit_recode_details_tvMemCodeTitle"
                android:layout_marginLeft="10dp"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white_ffffff"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >
            <TextView
                android:id="@+id/deposit_recode_details_tvOrderIdTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="订  单  号"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                />
            <TextView 
                android:id="@+id/deposit_recode_details_tvOrderId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/deposit_recode_details_tvOrderIdTitle"
                android:layout_marginLeft="10dp"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white_ffffff"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="充值金额"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/deposit_recode_details_tvTotal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </RelativeLayout>

        <View style="@style/divider_line1dp"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white_ffffff"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="充值时间"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                />
            <TextView 
                android:id="@+id/deposit_recode_details_tvPayTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <RelativeLayout 
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white_ffffff"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="支付方式"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                />
            <TextView 
                android:id="@+id/deposit_recode_details_tvPayWay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/white_ffffff"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="支付状态"
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/deposit_recode_details_tvOrderState"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/text_size14"
                android:textColor="@color/black_333"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <Button
			    android:id="@+id/deposit_recode_details_btOk"
			    android:text="完成"
                style="@style/button_styles"
                android:layout_marginTop="10dp"
                android:layout_margin="20dp"
			    ></Button>
        </LinearLayout>
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <Button
			    android:id="@+id/deposit_recode_details_btPrint"
			    android:text="详情补打"
                style="@style/button_styles"
                android:layout_marginTop="10dp"
                android:layout_margin="20dp"
			    ></Button>
        </LinearLayout>
    </LinearLayout>
	
</LinearLayout>
