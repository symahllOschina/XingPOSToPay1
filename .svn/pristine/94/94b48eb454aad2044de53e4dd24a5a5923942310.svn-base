<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white_ffffff"
    >
    <include layout="@layout/menu_title_layout"/>
    <RelativeLayout
        android:id="@+id/member_manage_queryLayout"
        style="@style/manage_menu_relativelayout"
        android:visibility="gone"
        >
        <ImageView
            android:id="@+id/member_manage_queryImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/member_query_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="会员查询"
            android:layout_toRightOf="@id/member_manage_queryImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:visibility="gone"
        />
    <RelativeLayout
        android:id="@+id/member_manage_topUpLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/member_manage_topUpImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/member_topup_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="会员充值"
            android:layout_toRightOf="@id/member_manage_topUpImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/member_manage_payLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/member_manage_payImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/member_pay_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="会员消费"
            android:layout_toRightOf="@id/member_manage_payImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/member_manage_writeOffLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/member_manage_writeOffImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/member_pay_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="卡劵核销"
            android:layout_toRightOf="@id/member_manage_writeOffImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />
    <RelativeLayout
        android:id="@+id/member_manage_buyCardLayout"
        style="@style/manage_menu_relativelayout"
        >
        <ImageView
            android:id="@+id/member_manage_buyCardImg"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/member_pay_icon"
            android:layout_centerVertical="true"
            />
        <TextView
            style="@style/manage_menu_textview"
            android:text="付费购卡"
            android:layout_toRightOf="@id/member_manage_buyCardImg"
            />
        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/next_icon"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            />
    </RelativeLayout>
    <View
        style="@style/divider_line1dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        />

</LinearLayout>
